{
  "editor.fontFamily": "'Monaspace Neon', 'IBM Plex Mono', monospace",
  "editor.fontSize": 14,
  "editor.lineHeight": 24,
  "editor.cursorStyle": "block",
  "editor.fontLigatures": "'calt', 'liga', 'dlig', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08'",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "editor.wordWrap": "bounded",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.showFoldingControls": "always",
  "editor.stickyScroll.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "always"
  },

  "window.commandCenter": true,
  "window.density.editorTabHeight": "default",

  "debug.toolBarLocation": "commandCenter",

  "emmet.includeLanguages": {
    "erb": "html",
    "ruby": "html"
  },

  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnSave": true
  },

  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.stickyScroll.enabled": true,

  "security.workspace.trust.untrustedFiles": "open",
  "gitlens.hovers.currentLine.over": "line",

  "workbench.startupEditor": "none",
  "workbench.editor.pinnedTabsOnSeparateRow": true,

  "typescript.tsserver.experimental.enableProjectDiagnostics": true,

  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],

  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "redhat.telemetry.enabled": false,
  "workbench.editorAssociations": {
    "*.tfplan": "default"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "terraform.experimentalFeatures.validateOnSave": true,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "githubPullRequests.pullBranch": "never",
  "git.suggestSmartCommit": false,
  "html.validate.styles": true,
  "workbench.panel.defaultLocation": "left",
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "sonarlint.rules": {
    "typescript:S4326": {
      "level": "on"
    },
    "typescript:S1440": {
      "level": "on"
    },
    "typescript:S909": {
      "level": "on"
    },
    "typescript:S4139": {
      "level": "on"
    },
    "typescript:S131": {
      "level": "on"
    },
    "typescript:S1066": {
      "level": "on"
    },
    "typescript:S134": {
      "level": "on"
    },
    "typescript:S121": {
      "level": "off"
    },
    "typescript:S3513": {
      "level": "on"
    },
    "typescript:S1535": {
      "level": "on"
    },
    "typescript:S126": {
      "level": "on"
    },
    "typescript:S3003": {
      "level": "on"
    },
    "typescript:S1541": {
      "level": "on"
    },
    "typescript:S138": {
      "level": "on"
    },
    "typescript:S103": {
      "level": "off"
    },
    "typescript:S881": {
      "level": "on"
    },
    "typescript:S109": {
      "level": "off"
    },
    "typescript:S6544": {
      "level": "on"
    },
    "typescript:S4157": {
      "level": "on"
    }
  },
  "git.openRepositoryInParentFolders": "never",
  "workbench.iconTheme": "twemoji-icon-theme",
  "[erb]": {
    "editor.defaultFormatter": "aliariff.vscode-erb-beautify",
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.html.erb": "erb",
    "*.env.development": "env",
    "*.env.production": "env"
  },
  "[ruby]": {
    "editor.defaultFormatter": "Shopify.ruby-lsp",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnType": true
  },
  "[html]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "telemetry.telemetryLevel": "off",
  "editor.largeFileOptimizations": true,
  "diffEditor.experimental.showMoves": true,
  "sonarlint.connectedMode.connections.sonarcloud": [],
  "githubPullRequests.terminalLinksHandler": "github",
  "editor.wordWrapColumn": 120,
  "workbench.editor.enablePreview": false,
  "workbench.productIconTheme": "feather-vscode",
  "sonarlint.pathToNodeExecutable": "/Users/glenntippett/.nvm/versions/node/v21.7.1/bin/node",
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "workbench.colorTheme": "Ayu Mirage Bordered"
}
